<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="Functional" preserve-order="true" configfailurepolicy="continue" parallel="tests" thread-count="2">
<listeners>
    <listener class-name="org.uncommons.reportng.HTMLReporter" />
 </listeners>
	<test name="Сопроводительные документы" >
