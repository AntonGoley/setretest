	  	<class name="ru.crystals.set10.test.tablereports.MRCPriceReportTest"/>
	  	<class name="ru.crystals.set10.test.tablereports.MRCPriceReportWithLekondsTest"/>
		</classes>	 
  	</test>	
		<test name="Выгрузка в SAP алкогольных ограничений" >
	  	<classes>
	  		<class name="ru.crystals.set10.test.SpiritRestrictionsToSAPTest"/>
		</classes>	 
  	</test>	 
</suite> 
